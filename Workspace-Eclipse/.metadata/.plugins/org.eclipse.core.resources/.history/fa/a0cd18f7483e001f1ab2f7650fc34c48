package day43;

import org.testng.Assert;
import org.testng.annotations.Test;

public class HardVsSoftAssertions {

	
	@Test
	void test_hardassertions()
	{
		System.out.println("Testing....");
		System.out.println("Testing....");
		System.out.println("Testing....");
		
		Assert.assertEquals(1, 1);
		
		System.out.println("Testing....");
		System.out.println("Testing....");
		System.out.println("Testing....");
	}
	
	
	
}
